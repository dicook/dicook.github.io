      function initialize(kml) {
      var myLatlng = new google.maps.LatLng(41.875696,-87.624207);
      var myOptions = {
      zoom: 8,
      center: myLatlng,
      mapTypeId: google.maps.MapTypeId.ROADMAP
      }
      var map = new google.maps.Map(document.getElementById("map_canvas"),
      myOptions);
      var Layer = new  google.maps.KmlLayer(kml); 
      Layer.setMap(map); 
      };
      function getKml(){
      var kml=document.getElementById('sel').value;
      initialize(kml)
      }
